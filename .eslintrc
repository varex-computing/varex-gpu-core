{
	"extends": "eslint-config-walmart/configurations/es6-node",
	"parserOptions": {
		"sourceType": "module"
	},
	"plugins": [
		"import"
	],
	"rules": {
		"strict": 0,
		"quotes": [
			2,
			"backtick"
		],
		"import/no-unresolved": 2,
		"filenames/match-regex": [2, "[A-Z][a-zA-Z0-9]", true],
		"consistent-this": [
			2,
			"_this"
		],
		"no-trailing-spaces": 0,
		"no-unused-vars": 1,
		"arrow-parens": 0,
		"comma-dangle": 0,
		"eqeqeq": [
			2,
			"always",
			{
				"null": "never"
			}
		],
		"consistent-return": 0,
	}
}